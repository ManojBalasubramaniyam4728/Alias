pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo "Building the code..."
                dir('alias') {  // Navigate to the correct directory
                    bat "mvn clean"
                }
            }
        }
        stage('Compile') {
            steps {
                echo "Compiling the code..."
                dir('alias') {  // Navigate to the correct directory
                    bat "mvn compile"
                }
            }
        }
        stage('Test') {
            steps {
                echo "Testing the code..."
                dir('alias') {  // Navigate to the correct directory
                    bat "mvn test"
                }
            }
        }
    }
}